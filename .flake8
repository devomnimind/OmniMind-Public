[flake8]
max-line-length = 100
extend-ignore = E203,W503
exclude =
    .git,
    __pycache__,
    .venv,
    tmp_agents,
    tmp_tools,
    tests/legacy,
    DEVBRAIN_V23,
    third_party,
    archive,
    data,
    src/agents/react_agent_broken.py,
    src/agents/react_agent.py.backup,
    src/integrations,
    src/onboarding
